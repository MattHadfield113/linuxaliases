alias sailinstall80='docker run --rm -u "$(id -u):$(id -g)" -v $(pwd):/var/www/html -w /var/www/html laravelsail/php80-composer:latest composer require laravel/sail --dev --ignore-platform-reqs && composer install --ignore-platform-reqs && php artisan sail:install'
